<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark">

	<fx:Script>
		<![CDATA[
			
//			[Embed(source="/images/beer-area.png")]
//			private static const BEER_AREA_PNG:Class;
			
		]]>
	</fx:Script>
	
	<fx:Metadata>
		[HostComponent("spark.components.Image")]
	</fx:Metadata>
	
	<s:states>
		<s:State name="disabled" />
		<s:State name="invalid" />
		<s:State name="ready" />
		<s:State name="loading" />
		<s:State name="uninitialized" />
	</s:states>
	
	<s:Rect left="0" top="0" right="0" bottom="0" radiusX="4" radiusY="4">
		<s:stroke>
			<s:SolidColorStroke color="#DDDDDD" />
		</s:stroke>
		<s:fill>
			<s:SolidColor color="#FFFFFF" />
		</s:fill>
	</s:Rect>
	
	<s:Rect left="5" top="5" right="5" bottom="5">
		<s:fill>
			<s:SolidColor color="#EEEEEE" />
		</s:fill>
	</s:Rect>
	
	<!--<s:BitmapImage  horizontalCenter="0" verticalCenter="0" />-->
	
	<s:BitmapImage id="imageDisplay" left="5" top="5" right="5" bottom="5" />
	
	<!-- SkinParts
	name=progressIndicator, type=spark.components.supportClasses.Range, required=false
	name=imageDisplay, type=spark.primitives.BitmapImage, required=true
	-->
</s:Skin>
