<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark">
	
	<fx:Metadata>
		[HostComponent("spark.components.TextInput")]
	</fx:Metadata>
	
	<s:states>
		<s:State name="disabledWithPrompt" />
		<s:State name="normalWithPrompt" />
		<s:State name="disabled" />
		<s:State name="normal" />
	</s:states>
	
	<s:Rect left="0" top="0" right="0" bottom="0">
		<s:fill>
			<s:SolidColor color="#FFFFFF" />
		</s:fill>
	</s:Rect>
	
	<s:Line left="0" top="0" right="0">
		<s:stroke>
			<s:SolidColorStroke color="#C0C0C0" weight="1" />
		</s:stroke>
	</s:Line>
	
	<s:Line top="0" right="0" bottom="0">
		<s:stroke>
			<s:SolidColorStroke color="#D9D9D9" weight="1" />
		</s:stroke>
	</s:Line>
	
	<s:Line left="0" right="0" bottom="0">
		<s:stroke>
			<s:SolidColorStroke color="#D9D9D9" weight="1" />
		</s:stroke>
	</s:Line>
	
	<s:Line left="0" top="0" bottom="0">
		<s:stroke>
			<s:SolidColorStroke color="#D9D9D9" weight="1" />
		</s:stroke>
	</s:Line>
	
	<s:RichEditableText id="textDisplay" verticalAlign="middle"
						widthInChars="10"
						left="2" right="2" top="1" bottom="1" />
	
	<s:Label id="promptDisplay" maxDisplayedLines="1" verticalAlign="middle" mouseEnabled="false" mouseChildren="false" left="2" right="2" top="1" bottom="1"
			 includeIn="normalWithPrompt,disabledWithPrompt" 
			/>
	
	<!-- SkinParts
	name=promptDisplay, type=spark.core.IDisplayText, required=false
	name=textDisplay, type=spark.core.IEditableText, required=false
	-->
</s:Skin>
